{
  "name": "ccview",
  "displayName": "CCView - Computational Chemistry File Viewer",
  "description": "Advanced 3D molecular visualization and analysis for VS Code. View quantum chemistry output files in stunning 3D, analyze molecular data with integrated terminal commands, and export results in multiple formats.",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Visualization",
    "Science"
  ],
  "keywords": [
    "chemistry",
    "molecular",
    "visualization",
    "quantum",
    "computational",
    "cclib",
    "miew",
    "gaussian",
    "gamess",
    "nwchem",
    "qchem",
    "psi4",
    "turbomole",
    "molpro",
    "molcas",
    "orca",
    "pdb",
    "xyz",
    "structure",
    "analysis"
  ],
  "publisher": "yamnor",
  "repository": {
    "type": "git",
    "url": "https://github.com/yamnor/ccview.git"
  },
  "homepage": "https://github.com/yamnor/ccview#readme",
  "bugs": {
    "url": "https://github.com/yamnor/ccview/issues"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ccview.openViewer",
        "title": "Open CCView",
        "category": "CCView"
      },
      {
        "command": "ccview.parseFile",
        "title": "Parse Computational Chemistry File",
        "category": "CCView"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "ccview.openViewer",
          "group": "navigation",
          "when": "resourceExtname == .log || resourceExtname == .out || resourceExtname == .pdb || resourceExtname == .cif || resourceExtname == .xyz"
        }
      ]
    },
    "files.associations": {
      "*.log": "plaintext",
      "*.out": "plaintext",
      "*.pdb": "plaintext",
      "*.cif": "plaintext",
      "*.xyz": "plaintext"
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0"
  },
  "license": "MIT"
}
